  <DataSets>
    <DataSet Name="Industry">
      <Query>
        <DataSourceName>MSSQL2017</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Ind_Top">
            <Value>=Parameters!Ind_Top.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Ind_Days">
            <Value>=Parameters!Ind_Days.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select top (@Ind_Top) [2017],left(Industry,10) Industry from (
select * from (
select ne1.Change Chg,year(ne1.Trnx_date) Yrs,ls1.Industry from NSE_EOD ne1 inner join ListOfScrips ls1 on
ne1.Script_Name = ls1.[Scrip Id]
where ne1.Trnx_date &gt; GETDATE() -@Ind_Days
) as T1

pivot
(
sum(Chg) for [Yrs] in ("2017") 
) as P1

) as B1
order by 1 desc</CommandText>
      </Query>
      <Fields>
        <Field Name="ID2017">
          <DataField>2017</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="Industry">
          <DataField>Industry</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
